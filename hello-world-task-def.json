{
    "family": "hello-world",
    "containerDefinitions": [
        {
            "name": "hello-world",
            "image": "456843195142.dkr.ecr.ap-northeast-1.amazonaws.com/hello-world",
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "entryPoint": [
                "/usr/sbin/apache2",
                "-D",
                "FOREGROUND"
            ],
            "essential": true
        }
    ]
}
